{
  "name": "ycom",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/react-hooks": "^3.3.0",
    "@testing-library/user-event": "^7.1.2",
    "eslint": "^7.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.20.0",
    "moment": "^2.26.0",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-google-charts": "^3.0.15",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "1.1.5",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:ssr": "node ./server/",
    "build": "react-scripts build && yarn run ssr",
    "ssr": "cross-env NODE_ENV=production webpack --config ./webpack.ssr.config.js",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "CI=true yarn test --coverage -u",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "yarn prettier --write src/",
    "postbuild": "find ./build -exec gzip -k '{}' \\;"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "cross-env": "^7.0.2",
    "husky": "^4.2.5",
    "jest-localstorage-mock": "^2.4.2",
    "lint-staged": "^10.2.9",
    "react-test-renderer": "^16.13.1",
    "url-search-params-polyfill": "^8.1.0",
    "whatwg-fetch": "^3.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/index.js",
      "!<rootDir>/src/App.jsx"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "[[ -n $HUSKY_BYPASS ]] || commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src": [
      "yarn lint:fix",
      "yarn prettier",
      "git add"
    ]
  }
}
